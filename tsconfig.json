{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "lib": ["ES2022", "DOM", "DOM.Iterable"],
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "allowJs": true,
    "isolatedModules": true,
    "moduleDetection": "force",
    "useDefineForClassFields": false,
    "skipLibCheck": true,
    "noEmit": true,
    "types": ["vite/client", "node"],
  
    "paths": {
      "@/*": ["./*"]
    }
  }
}
